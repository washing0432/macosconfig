// Place your key bindings in this file to overwrite the defaults
[
    {
        "key": "ctrl+h",
        "command": "cursorLeft",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+j",
        "command": "cursorDown",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+k",
        "command": "cursorUp",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+l",
        "command": "cursorRight",
        "when": "editorTextFocus"
    },
    { 
        "key": "cmd+l",                 
        "command": "expandLineSelection",
        "when": "editorTextFocus" 
    }
]